{
  "name": "linter-jolie",
  "description": "A Jolie linter for Atom",
  "version": "0.7.1",
  "main": "./lib/main",
  "configSchema": {
    "jolieExecutablePath": {
      "type": "string",
      "default": "jolie",
      "title": "Jolie Executable Path",
      "description": "The executable path to Jolie. In most systems, the default works if Jolie is in your PATH environment variable. If it does not work for you, put the full path here, e.g., /usr/bin/jolie"
    }
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.6.0"
  },
  "package-deps": [
    "language-jolie",
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "homepage": "https://github.com/fmontesi/linter-jolie",
  "repository": {
    "type": "git",
    "url": "https://github.com/fmontesi/linter-jolie.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fmontesi/linter-jolie/issues"
  }
}
